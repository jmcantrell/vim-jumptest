Execute (jump to test):
  edit ./foo.py
  call jumptest#jump()
  AssertEqual expand('%'), './foo.test.py'

Execute (jump to main):
  edit ./foo.test.py
  call jumptest#jump()
  AssertEqual expand('%'), './foo.py'

Execute (relative jump to test):
  edit ./path/to/foo.py
  call jumptest#jump()
  AssertEqual expand('%'), './path/to/foo.test.py'

Execute (relative jump to main):
  edit ./path/to/foo.test.py
  call jumptest#jump()
  AssertEqual expand('%'), './path/to/foo.py'

Execute (absolute jump to test):
  edit /path/to/foo.py
  call jumptest#jump()
  AssertEqual expand('%'), '/path/to/foo.test.py'

Execute (absolute jump to main):
  edit /path/to/foo.test.py
  call jumptest#jump()
  AssertEqual expand('%'), '/path/to/foo.py'
